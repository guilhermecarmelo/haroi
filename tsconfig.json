{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"declaration": true,
		"esModuleInterop": true,
		"module": "CommonJS",
		"moduleResolution": "Node",
		"outDir": "./lib",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": false,
		"target": "ES6"
	},
	"include": ["./src"]
}
